{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Base",
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "stripInternal": true,
    "outDir": "dist",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": false
  }
}

{
  "name": "internals/shared-metaplex",
  "version": "0.6.5",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --config build-configs/tsup/base.ts",
    "clean": "rm -rf dist/ && rm -rf node_modules",
    "eslint": "eslint -c node_modules/build-configs/eslint/base.json ./src ./test",
    "eslint:fix": "eslint -c node_modules/build-configs/eslint/base.json ./src ./test --fix",
    "prettier": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c",
    "prettier:fix": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c --write",
    "test": "mocha -t 300000 --require ts-node/register -- ",
    "test:all": "mocha -t 300000 --require ts-node/register './test/**/*.test.ts'"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^2.9.1",
    "@solana-suite/shared": "workspace:^",
    "@solana/web3.js": "^1.73.2",
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/jsdom": "^21.1.3",
    "build-configs/eslint": "workspace:*",
    "build-configs/prettier": "workspace:*",
    "build-configs/tsconfig": "workspace:*",
    "build-configs/tsup": "workspace:*",
    "jsdom": "^22.1.0"
  }
}

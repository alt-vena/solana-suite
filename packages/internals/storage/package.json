{
  "name": "internals/storage",
  "version": "0.6.5",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsup --config build-configs/tsup/base.ts",
    "clean": "rm -rf dist/ && rm -rf node_modules",
    "test": "jest -c node_modules/build-configs/jest/base.ts --rootDir .",
    "lint": "eslint -c node_modules/build-configs/eslint/base.json ./src/*",
    "style:fix": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c --write"
  },
  "dependencies": {
    "@metaplex-foundation/js": "0.18.0",
    "internals/shared-metaplex": "workspace:^",
    "@solana-suite/shared": "workspace:^",
    "nft.storage": "^7.1.1"
  },
  "devDependencies": {
    "build-configs/eslint": "workspace:*",
    "build-configs/jest": "workspace:*",
    "build-configs/prettier": "workspace:*",
    "build-configs/tsconfig": "workspace:*",
    "build-configs/tsup": "workspace:*"
  },
  "browser": {
    "fs": false
  },
  "node": {
    "File": false
  }
}

{
  "name": "@solana-suite/shared",
  "version": "0.6.5",
  "description": "shared module for solana-suite",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "api",
    "blockchain",
    "solana",
    "sdk",
    "web3.js",
    "nft"
  ],
  "engines": {
    "node": ">=16.*"
  },
  "files": [
    "dist",
    "solana-suite-config.js",
    "package.json"
  ],
  "license": "MIT",
  "author": "Akira Takachi <a.takachi@atonoy.co>",
  "homepage": "https://atonoy.github.io/solana-suite",
  "repository": {
    "type": "git",
    "url": "https://github.com/atonoy/solana-suite.git"
  },
  "bugs": {
    "url": "https://github.com/atonoy/solana-suite/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --config build-configs/tsup/base.ts && cp src/solana-suite.json dist/",
    "clean": "rm -rf dist/ && rm -rf node_modules",
    "test": "jest -c node_modules/build-configs/jest/base.ts --rootDir . -- ",
    "eslint": "eslint -c node_modules/build-configs/eslint/base.json ./src ./test",
    "prettier": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c",
    "prettier:fix": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c --write",
    "style": "pnpm prettier && pnpm eslint"
  },
  "dependencies": {
    "@solana/web3.js": "^1.78.5",
    "bignumber.js": "^9.1.2",
    "bs58": "^5.0.0",
    "commander": "9.3.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.3",
    "build-configs/eslint": "workspace:*",
    "build-configs/jest": "workspace:*",
    "build-configs/prettier": "workspace:*",
    "build-configs/tsconfig": "workspace:*",
    "build-configs/tsup": "workspace:*",
    "jsdom": "^22.1.0"
  }
}

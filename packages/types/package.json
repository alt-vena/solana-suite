{
  "name": "types",
  "version": "0.7.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "./dist/"
  ],
  "scripts": {
    "build": "tsup --config ./tsupconfig.ts",
    "clean": "rm -rf dist/ && rm -rf node_modules",
    "eslint": "eslint -c node_modules/build-configs/eslint/base.json ./src ./test",
    "eslint:fix": "eslint -c node_modules/build-configs/eslint/base.json ./src ./test --fix",
    "prettier": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c",
    "prettier:fix": "prettier --config node_modules/build-configs/prettier/base.json ./src/ ./test/ -c --write"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
    "@solana-suite/shared": "workspace:*",
    "@solana/web3.js": "^1.78.5",
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.2",
    "build-configs/eslint": "workspace:*",
    "build-configs/prettier": "workspace:*",
    "build-configs/tsconfig": "workspace:*",
    "build-configs/tsup": "workspace:*"
  },
  "exports": {
    "./converter": {
      "require": "./dist/converter.js",
      "import": "./dist/converter.mjs",
      "types": "./dist/converter.d.ts"
    },
    "./storage": {
      "require": "./dist/storage.js",
      "import": "./dist/storage.mjs",
      "types": "./dist/storage.d.ts"
    },
    "./validator": {
      "require": "./dist/validator.js",
      "import": "./dist/validator.mjs",
      "types": "./dist/validator.d.ts"
    }
  }
}
